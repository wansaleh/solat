    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "cheerio": "^1.0.0-rc.10",
    "date-fns": "^2.28.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.1",
    "svelte-dark-mode": "^2.0.0",
    "svelte-geolocation": "^0.3.0",
    "svelte-preprocess": "^4.10.1",
    "svelte-time": "^0.6.1",
